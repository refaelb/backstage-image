    "lodash": "^4.17.21",
    "qs": "^6.10.1",
    "winston": "^3.2.1",
    "yn": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@backstage/backend-test-utils": "^0.2.10",
    "@backstage/cli": "^0.25.1",
    "@types/supertest": "^2.0.8",
