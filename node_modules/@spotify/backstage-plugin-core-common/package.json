{
  "name": "@spotify/backstage-plugin-core-common",
  "description": "Isomorphic utilities for Spotify Plugins for Backstage",
  "version": "0.5.9",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://backstage.spotify.com",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "common-library"
  },
  "sideEffects": false,
  "scripts": {
